        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a69" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>Five o'clock in the morning, right?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a70" ANNOTATION_REF="a40">
                <ANNOTATION_VALUE>You...</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a71" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>The bridge over the Dnepr, right?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a72" ANNOTATION_REF="a30">
                <ANNOTATION_VALUE>A crossing?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a73" ANNOTATION_REF="a31">
                <ANNOTATION_VALUE>Bombs?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a78" ANNOTATION_REF="a37">
                <ANNOTATION_VALUE>Was he dirty?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a79" ANNOTATION_REF="a38">
                <ANNOTATION_VALUE>His clothes were torn...</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
